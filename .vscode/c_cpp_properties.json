{
  "configurations": [
    {
      "name": "Win32",
      "compilerPath": "${env:AppData}\\mips\\mips\\bin\\mipsel-none-elf-gcc.exe",
      "cStandard": "c17",
      "cppStandard": "c++20",
      "defines": [
        "__STDC_HOSTED__ = 0",
        "XM_BUILTIN",
        "XM_DIR",
        "XM_FILENAME",
        "XM_TYPE",
        "XM_LOOP",
        "XM_POSITION",
        "XM_PANNING_TYPE",
        "MAX_PXM_SIZE",
        "MAX_VH_SIZE",
        "MAX_VB_SIZE"
      ],
      "includePath": [
        "${env:AppData}/mips/mips/include",
        "${workspaceFolder}/psexe/",
        "${workspaceFolder}/psexe/nugget",
        "${workspaceFolder}/psexe/xmplay/include",
        "${workspaceFolder}/psexe/psyq/include"
      ],
      "intelliSenseMode": "gcc-x86"
    },
    {
      "name": "linux",
      "compilerPath": "mipsel-linux-gnu-gcc",
      "cStandard": "c17",
      "cppStandard": "c++20",
      "defines": [
        "__STDC_HOSTED__ = 0",
        "XM_BUILTIN",
        "XM_DIR",
        "XM_FILENAME",
        "XM_TYPE",
        "XM_LOOP",
        "XM_POSITION",
        "XM_PANNING_TYPE",
        "MAX_PXM_SIZE",
        "MAX_VH_SIZE",
        "MAX_VB_SIZE"
      ],
      "includePath": [
        "/usr/mipsel-linux-gnu/include",
        "/usr/local/mipsel-linux-gnu/include",
        "/usr/mipsel-none-elf/include",
        "/usr/local/mipsel-none-elf/include",
        "${workspaceFolder}/psexe/",
        "${workspaceFolder}/psexe/nugget",
        "${workspaceFolder}/psexe/xmplay/include",
        "${workspaceFolder}/psexe/psyq/include"
      ],
      "intelliSenseMode": "gcc-x86"
    }
  ],
  "version": 4
}